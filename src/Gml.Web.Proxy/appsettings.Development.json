{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Clusters": {
      "backend": {
        "Destinations": {
          "backend/d1": {
            "Address": "http://localhost:5000"
          }
        },
        "HealthCheck": {
          "Active": {
            "Enabled": true,
            "Interval": "00:00:10",
            "Timeout": "00:00:05",
            "Policy": "ConsecutiveFailures",
            "Path": "/health"
          },
          "Passive": {
            "Enabled": true,
            "Policy": "TransportFailureRate",
            "ReactivationPeriod": "00:00:30"
          }
        }
      },
      "skins": {
        "Destinations": {
          "skins/d1": {
            "Address": "http://localhost:8085"
          }
        }
      },
      "frontend": {
        "Destinations": {
          "frontend/d1": {
            "Address": "https://localhost:3000"
          }
        }
      }
    }
  }
}
